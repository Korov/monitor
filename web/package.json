    "element-plus": "^2.3.9",
    "pinia": "^2.1.6",
    "vue": "^3.3.4",
    "vue-axios": "^3.5.2",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.2.4",
    "vxe-table": "^4.4.7"
  },
  "devDependencies": {
    "@types/node": "^20.5.0",
    "@typescript-eslint/eslint-plugin": "^5.30.4",
    "@vitejs/plugin-vue": "^4.2.3",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
