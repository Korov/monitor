    "vue-i18n": "^9.8.0",
    "vue-router": "^4.2.5",
    "vxe-table": "^4.5.17"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@vitejs/plugin-vue": "^5.0.0",
    "eslint": "^8.56.0",
