{
  "name": "web",
  "version": "0.0.0",
  "description": "",
  "author": "",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "vite --host",
    "build": "vue-tsc --noEmit && vite build",
    "build:vite": "vite build",
    "preview": "vite preview",
    "lint": "eslint src --ext .js,.ts",
