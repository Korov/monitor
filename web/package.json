    "up": "npx npm-check-updates -u"
  },
  "dependencies": {
    "axios": "^1.2.0",
    "core-js": "^3.26.1",
    "element-plus": "^2.2.25",
    "pinia": "^2.0.14",
    "vue": "^3.2.37",
    "vue-axios": "^3.5.2",
    "vue-router": "^4.0.16",
    "vxe-table": "^4.3.6"
  },
  "devDependencies": {
    "@types/node": "^18.0.1",
    "@typescript-eslint/eslint-plugin": "^5.30.4",
    "@typescript-eslint/parser": "^5.30.4",
    "@vitejs/plugin-vue": "^2.3.3",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.1.1",
    "less": "^4.1.3",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "sass": "^1.56.1",
    "sass-loader": "^13.2.0",
    "typescript": "^4.7.4",
    "unplugin-auto-import": "^0.12.0",
    "unplugin-vue-components": "^0.21.2",
    "vite": "^2.9.13",
    "vite-plugin-banner": "^0.3.0",
    "vue-tsc": "^1.0.9",
    "yorkie": "^2.0.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged --concurrent false"
  },
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  }
}
